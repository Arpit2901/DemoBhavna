package calcAreaShape;

public class shapeArea {
	public void area(int l,int b) {
		System.out.print("th area of rectangle"+ l*b);
		
	}
	public void area(int s) {
		System.out.print("th area of rectangle"+ s*s);
	}
	public void area(double r) {
		System.out.print("th area of rectangle"+3.141*r*r);
	}

}
