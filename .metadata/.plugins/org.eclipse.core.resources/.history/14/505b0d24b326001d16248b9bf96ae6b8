package interestCalci;

public class calculateInterest {
	private int r,t,age,in;
	private long p;
	private char gender;
	
	public calculateInterest(int r, int t, int age, int in, long p, char gender) {
		super();
		this.r = r;
		this.t = t;
		this.age = age;
		this.in = in;
		this.p = p;
		this.gender = gender;
	}
	
	public void calSI() {
		int i=(p*r*t)/100;
	}
	public void calCI() {
		int i=(p*r*t)/100;
	}

}
