package prime;

import java.util.Scanner;

public class isPrime {

	public static void main(String[] args) {
		int i,z=0,j,n,ctr=0;
		Scanner sc =new Scanner(System.in);
		n=sc.nextInt();
		for(j=2;j<100;j++) {
		for(i=2;i<Math.sqrt(j);i++) {
		if(j%i==0) {
		ctr++;
		}
		}
		if(ctr==0) {
			z++;
		}
		if(z==n) {
			System.out.print("nth prime"+j);
			break;
		}
		}
	}

}
